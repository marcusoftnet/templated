{
  "name": "templated",
  "version": "1.0.0",
  "description": "The Templated view engine is a super simple, pure JavaScript view engines with few dependencies and using a syntax known to all JavaScript devs",
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "build": "npm run clean && tsc -p tsconfig.build.json && npm run minify",
    "minify": "terser dist/*.js --compress --mangle --output dist/templated.js",
    "postminify": "rm dist/errors.js dist/index.js"
  },
  "keywords": [
    "template",
    "template string",
    "view engine"
  ],
  "author": "Marcus Hammarberg",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.56.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "terser": "^5.31.3",
    "ts-jest": "^29.2.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  }
}
